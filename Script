local Button = UniversalTab:CreateButton({
   Name = "infinity yield",
   Callback = function()
   loadstring(game:HttpGet("https://rawscripts.net/raw/Infinite-Yield_500"))()
   end,
})

Rayfield:Notify({
   Title = "Notification",
   Content = "Mysterious hub Successfully loaded",
   Duration = 6.5,
   Image = 4483362458,
})

